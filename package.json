{
	"name": "@ky-is/vue-markdown-poi",
	"version": "0.2.1",
	"description": "A lightweight Vue component for rendering content with a markdown-like syntax subset.",
	"main": "src/index.vue",
	"author": "Kyle Coburn",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ky-is/vue-markdown-poi.git"
	},
	"keywords": [
		"vue",
		"markdown",
		"md"
	],
	"bugs": {
		"url": "https://github.com/ky-is/vue-markdown-poi/issues"
	},
	"homepage": "https://github.com/ky-is/vue-markdown-poi#readme",
	"scripts": {
		"test": "jest"
	},
	"devDependencies": {
		"@ky-is/eslint-config": "^1.5.1",
		"@vue/test-utils": "^1.0.0-beta.25",
		"babel-jest": "^23.6.0",
		"babel-preset-env": "^1.7.0",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^4.7.1",
		"jest": "^23.6.0",
		"vue": "^2.5.17",
		"vue-jest": "^3.0.0",
		"vue-template-compiler": "^2.5.17"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"modules": false
				}
			]
		],
		"env": {
			"test": {
				"presets": [
					[
						"env",
						{
							"targets": {
								"node": "current"
							}
						}
					]
				]
			}
		}
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/vue"
	},
	"jest": {
		"testURL": "http://localhost",
		"testMatch": [
			"<rootDir>/tests/**/*.spec.js"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.vue$": "<rootDir>/node_modules/vue-jest"
		},
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		}
	}
}
